  <h3><%= title %></h3>

  <% if users.present? %>
    <ul class="user-list" id="user-list">
      <%= render 'users/pagenate', users: users %>
    </ul>
    <%= link_to_next_page users, 'つづきを見る', remote: true, id: 'more_link' unless users.nil? %>
  <% else %>
    <h4 class="tac">投稿が見つかりませんでした。</h4>
  <% end %>
  